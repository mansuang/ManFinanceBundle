<?php

namespace Man\FinanceBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * CardFinance
 */
class CardFinance
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $full_price;

    /**
     * @var string
     */
    private $down_price;

    /**
     * @var string
     */
    private $expense_cost;

    /**
     * @var string
     */
    private $total_round;

    /**
     * @var string
     */
    private $total_costloan;

    /**
     * @var string
     */
    private $monthly;

    /**
     * @var string
     */
    private $late_round;

    /**
     * @var \Man\FinanceBundle\Entity\Card
     */
    private $card;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set full_price
     *
     * @param string $fullPrice
     * @return CardFinance
     */
    public function setFullPrice($fullPrice)
    {
        $this->full_price = $fullPrice;

        return $this;
    }

    /**
     * Get full_price
     *
     * @return string 
     */
    public function getFullPrice()
    {
        return $this->full_price;
    }

    /**
     * Set down_price
     *
     * @param string $downPrice
     * @return CardFinance
     */
    public function setDownPrice($downPrice)
    {
        $this->down_price = $downPrice;

        return $this;
    }

    /**
     * Get down_price
     *
     * @return string 
     */
    public function getDownPrice()
    {
        return $this->down_price;
    }

    /**
     * Set expense_cost
     *
     * @param string $expenseCost
     * @return CardFinance
     */
    public function setExpenseCost($expenseCost)
    {
        $this->expense_cost = $expenseCost;

        return $this;
    }

    /**
     * Get expense_cost
     *
     * @return string 
     */
    public function getExpenseCost()
    {
        return $this->expense_cost;
    }

    /**
     * Set total_round
     *
     * @param string $totalRound
     * @return CardFinance
     */
    public function setTotalRound($totalRound)
    {
        $this->total_round = $totalRound;

        return $this;
    }

    /**
     * Get total_round
     *
     * @return string 
     */
    public function getTotalRound()
    {
        return $this->total_round;
    }

    /**
     * Set total_costloan
     *
     * @param string $totalCostloan
     * @return CardFinance
     */
    public function setTotalCostloan($totalCostloan)
    {
        $this->total_costloan = $totalCostloan;

        return $this;
    }

    /**
     * Get total_costloan
     *
     * @return string 
     */
    public function getTotalCostloan()
    {
        return $this->total_costloan;
    }

    /**
     * Set monthly
     *
     * @param string $monthly
     * @return CardFinance
     */
    public function setMonthly($monthly)
    {
        $this->monthly = $monthly;

        return $this;
    }

    /**
     * Get monthly
     *
     * @return string 
     */
    public function getMonthly()
    {
        return $this->monthly;
    }

    /**
     * Set late_round
     *
     * @param string $lateRound
     * @return CardFinance
     */
    public function setLateRound($lateRound)
    {
        $this->late_round = $lateRound;

        return $this;
    }

    /**
     * Get late_round
     *
     * @return string 
     */
    public function getLateRound()
    {
        return $this->late_round;
    }

    /**
     * Set card
     *
     * @param \Man\FinanceBundle\Entity\Card $card
     * @return CardFinance
     */
    public function setCard(\Man\FinanceBundle\Entity\Card $card = null)
    {
        $this->card = $card;

        return $this;
    }

    /**
     * Get card
     *
     * @return \Man\FinanceBundle\Entity\Card 
     */
    public function getCard()
    {
        return $this->card;
    }
}
