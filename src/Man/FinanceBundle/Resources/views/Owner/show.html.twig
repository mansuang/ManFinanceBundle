{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Owner</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Owner_name</th>
                <td>{{ entity.ownername }}</td>
            </tr>
            <tr>
                <th>Is_default</th>
                <td>{{ entity.isdefault }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('finance_owner') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('finance_owner_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
